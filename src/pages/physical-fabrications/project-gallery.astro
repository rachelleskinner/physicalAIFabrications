---
import { Image } from 'astro:assets';
import 'bootstrap/dist/css/bootstrap.css'
import '../../styles/physical-fabrications/global.css';
import '../../styles/physical-fabrications/gallery.css';
---
<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/x-icon" href="/fabricationImgs/favicon.ico" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<meta name="generator" content={Astro.generator} />
		<title>DCD 300 Digital Gallery</title>
		<link href="css/hover-min.css" rel="stylesheet">
		<!--Bootstrap Stylesheet -->
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <!--Fonts -->
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Baloo+2:wght@400..800&display=swap" rel="stylesheet">
	</head>
	<body>

        <header>
            <div class="container">
                <div class="row justify-content-center text-center">
                    <div class="col">
                        <Image class="img-fluid mt-5" src="/fabricationImgs/fontHeaders/physicalAIFabrications.png" width="1753" height="422" alt="Physical & AI Fabrications in a playful, coloful, magazine cut-out letter font">
                    </div>
                </div>
                <div class="row justify-content-end">
                    <div class="col-md-6 text-start ps-4 w-50">
                        <h2 class="subHeaderText">A DCD 300 Digital Gallery</h2>
                    </div>
                    <div class="col-md-6 text-end pe-4 w-50">
                        <h2 class="subHeaderText">By Rachell Skinner</h2>
                    </div>
                </div>
            </div>
        </header>
        <br>
        <br>

        <!--Project Gallery -->
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-6 col-sm-6 d-flex align-items-center justify-content-center">
                    <a href="/physical-fabrications/project1-keywords" class="">
                        <h2 class="titleText">Project 1: Keywords & Background</h2>
                        <Image class="img-fluid" src="/fabricationImgs/fontHeaders/keywords.png" loading="eager" width="600" height="100.8" alt=""></Image>
                    </a>
                </div>
            </div>
            <br>
            <br>
            <br>
            
			<div class="row justify-content-center">
                <div class="col-md-4 col-sm-6 d-flex align-items-center justify-content-center p-3">
                    <a href="/physical-fabrications/project2-collage">
                        <h2 class="titleText">Project 2: Collage</h2>
                        <Image class="img-fluid" src="/fabricationImgs/p2-collage/collage.png" loading="eager" width="400" height="500" alt=""></Image>
                    </a>
                </div>
                <div class="col-md-4 col-sm-6 d-flex align-items-center justify-content-center p-3" >
                    <a href="/physical-fabrications/project3-artist-book">
                        <h2 class="titleText">Project 3: Artist Book</h2>
                        <Image class="img-fluid" src="/fabricationImgs/p3-artistBook/artistBook.png" width="400" height="500" alt=""></Image>
                    </a>
                </div>
                <div class="col-md-4 col-sm-6 d-flex align-items-center justify-content-center p-3">
                    <a href="/physical-fabrications/project4-font">
                        <h2 class="titleText">Project 4: Font</h2>
                        <Image class="img-fluid" src="/fabricationImgs/p4-font/font.png" width="400" height="500" alt=""></Image>
                    </a>
                </div>
            </div>

            <br>
            <br>
            <br>
            <div class="row justify-content-center">
                <div class="col-md-6 col-sm-6 d-flex align-items-center justify-content-center">
                    <a href="/physical-fabrications/statement" class="">
                        <h2 class="titleText">Gallery & Course Statement</h2>
                        <Image class="img-fluid" src="/fabricationImgs/fontHeaders/statement.png" loading="eager" width="700" height="105" alt=""></Image>
                    </a>
                </div>
            </div>
        </div>
        <br>
        <br>
        <br>

    <footer>
        <div class="container">
            <p>&copy; Rachell Skinner 2024</p>
        </div>
    </footer>

    </body>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</html>